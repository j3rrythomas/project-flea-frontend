{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],

  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": { "react/react-in-jsx-scope": "off" },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
